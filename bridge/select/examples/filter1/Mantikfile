# Filters columns, select * where x = 1
stack: select
type:
    input:
        columns:
            "x": int32
            "y": string
    output:
        columns:
            a: string
            b: int32
selectProgram:
    selector:
        args: 2
        retStackDepth: 1
        stackInitDepth: 1
        ops:
          - get
          - 0
          - cnt
          - { "type": "int32", "value": 1 }
          - eq
          - int32
    projector:
        args: 2
        retStackDepth: 2
        stackInitDepth: 2
        ops:
          - get
          - 0
